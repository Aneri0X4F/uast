#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aneri/Documents/codes/uast/node_modules/.pnpm/sirv-cli@2.0.2/node_modules/sirv-cli/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/sirv-cli@2.0.2/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/node_modules:/home/aneri/Documents/codes/uast/node_modules:/home/aneri/Documents/codes/node_modules:/home/aneri/Documents/node_modules:/home/aneri/node_modules:/home/node_modules:/node_modules:/home/aneri/Documents/codes/uast/node_modules/sirv-cli/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aneri/Documents/codes/uast/node_modules/.pnpm/sirv-cli@2.0.2/node_modules/sirv-cli/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/sirv-cli@2.0.2/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/node_modules:/home/aneri/Documents/codes/uast/node_modules:/home/aneri/Documents/codes/node_modules:/home/aneri/Documents/node_modules:/home/aneri/node_modules:/home/node_modules:/node_modules:/home/aneri/Documents/codes/uast/node_modules/sirv-cli/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sirv-cli/bin.js" "$@"
else
  exec node  "$basedir/../sirv-cli/bin.js" "$@"
fi
