#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/aneri/Documents/codes/uast/node_modules/.pnpm/livereload@0.9.3/node_modules/livereload/bin/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/livereload@0.9.3/node_modules/livereload/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/livereload@0.9.3/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/node_modules:/home/aneri/Documents/codes/uast/node_modules:/home/aneri/Documents/codes/node_modules:/home/aneri/Documents/node_modules:/home/aneri/node_modules:/home/node_modules:/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/rollup-plugin-livereload@2.0.5/node_modules/rollup-plugin-livereload/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/rollup-plugin-livereload@2.0.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/aneri/Documents/codes/uast/node_modules/.pnpm/livereload@0.9.3/node_modules/livereload/bin/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/livereload@0.9.3/node_modules/livereload/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/livereload@0.9.3/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/node_modules:/home/aneri/Documents/codes/uast/node_modules:/home/aneri/Documents/codes/node_modules:/home/aneri/Documents/node_modules:/home/aneri/node_modules:/home/node_modules:/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/rollup-plugin-livereload@2.0.5/node_modules/rollup-plugin-livereload/node_modules:/home/aneri/Documents/codes/uast/node_modules/.pnpm/rollup-plugin-livereload@2.0.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../livereload@0.9.3/node_modules/livereload/bin/livereload.js" "$@"
else
  exec node  "$basedir/../../../../../livereload@0.9.3/node_modules/livereload/bin/livereload.js" "$@"
fi
